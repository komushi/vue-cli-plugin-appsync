{
    "dataSources": [
        {
            "name": "BookTable",
            "description": null,
            "type": "AMAZON_DYNAMODB",
            "serviceRoleArn": "{managed-by-awsmobile-cli}",
            "dynamodbConfig": {
                "tableName": "BookTable-{suffix}",
                "useCallerCredentials": false
            }
        }
    ],
    "tables": [
        {
            "AttributeDefinitions": [
                {
                    "AttributeName": "title",
                    "AttributeType": "S"
                }
            ],
            "TableName": "BookTable-{suffix}",
            "KeySchema": [
                {
                    "AttributeName": "title",
                    "KeyType": "HASH"
                }
            ],
            "ProvisionedThroughput": {
                "ReadCapacityUnits": 5,
                "WriteCapacityUnits": 5
            }
        }
    ]
}